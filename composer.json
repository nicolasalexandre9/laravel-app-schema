{
    "name": "nicolasalexandre9/laravel-app-schema",
    "description": "Introspect models of your laravel application",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexandre Nicolas",
            "email": "nicolasalexandre9@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^8.0",
        "doctrine/dbal": "^3.1.1"
    },
    "autoload": {
        "psr-4": {
            "Nicolasalexandre9\\LaravelAppSchema\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nicolasalexandre9\\LaravelAppSchema\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nicolasalexandre9\\LaravelAppSchema\\LaravelAppSchemaServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "9.5.10"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
